{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^2.18.21",
    "gatsby-image": "^2.2.38",
    "gatsby-plugin-google-analytics": "^2.1.32",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-netlify-cms": "^4.1.35",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-offline": "^3.0.31",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.12",
    "netlify-cms-app": "^2.11.1",
    "node-sass": "^4.13.0",
    "prismjs": "^1.18.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-notes": "^5.3.0",
    "@storybook/addon-viewport": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "babel-preset-gatsby": "^0.2.27",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "current-git-branch": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^4.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "onchange": "^6.1.0",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^12.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env FORCE_COLOR=true concurrently --raw \"npm:test\" \"lint-staged\"",
      "pre-push": "node ./scripts/branchCheck.js"
    }
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern /src/vendor/*",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss --color --allow-empty-input --fix",
      "git add"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "concurrently --raw \"npm:lint --silent\" \"npm:test\" && gatsby build",
    "develop": "cross-env NODE_ENV=development concurrently --raw \"npm:lint --silent\" \"gatsby develop --port 3000\" \"npm:watch --silent\"",
    "lint": "concurrently --raw \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:fix": "concurrently --raw \"eslint ./*.js src/**/*.js .storybook/*.js --fix\" \"stylelint src/**/*.scss --syntax scss --color --allow-empty-input --fix\"",
    "lint:js": "eslint ./*.js src/**/*.js .storybook/*.js --ignore-pattern /src/vendor/*",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --color --allow-empty-input",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "cross-env NODE_ENV=production concurrently --raw \"start-storybook --port 6060\" \"npm:watch --silent\"",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o storybook-dist",
    "test": "jest src/",
    "watch": "concurrently --raw \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent",
    "watch:test": "jest --watch src/"
  },
  "version": "1.0.0"
}
