{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^5.12.3",
    "gatsby-image": "^3.6.0",
    "gatsby-plugin-google-analytics": "^5.12.0",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-netlify-cms": "^7.12.0",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-offline": "^6.12.0",
    "gatsby-plugin-postcss": "^6.12.0",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sass": "^6.12.0",
    "gatsby-plugin-sharp": "^5.12.0",
    "gatsby-remark-images": "^7.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-remark": "^6.12.0",
    "gatsby-transformer-sharp": "^5.12.0",
    "netlify-cms-app": "^2.15.11",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.4.3"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^7.0.2",
    "@storybook/addons": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "babel-jest": "^29.6.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "babel-preset-gatsby": "^3.12.0",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "current-git-branch": "^1.1.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "ftp": "^0.3.10",
    "gatsby-plugin-sitemap": "^6.12.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "stylelint": "^15.10.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^6.0.1",
    "stylelint-scss": "^5.1.0"
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern /src/vendor/*"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "concurrently --raw \"npm:lint:fix --silent\" \"npm:test\" && gatsby build",
    "build:deploy": "npm run build && npm run migrate:artifacts",
    "deploy": " node -r dotenv/config ./scripts/deploy.js",
    "develop": "cross-env NODE_ENV=development cross-env GATSBY_GRAPHQL_IDE=playground concurrently --raw \"npm:lint --silent\" \"gatsby develop --port 3000\" \"npm:watch --silent\"",
    "lint": "concurrently --raw \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:fix": "concurrently --raw \"eslint ./*.js src/**/*.js .storybook/*.js --fix\" \"stylelint src/**/*.scss --color --allow-empty-input --fix\"",
    "lint:js": "eslint ./*.js src/**/*.js .storybook/*.js --ignore-pattern /src/vendor/*",
    "lint:scss": "stylelint src/**/*.scss --color --allow-empty-input",
    "migrate:artifacts": "node ./scripts/migrateArtifacts.js",
    "pre-commit": "cross-env FORCE_COLOR=true concurrently --raw \"npm:test\" \"lint-staged\"",
    "pre-push": "node ./scripts/branchCheck.js",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/*.scss\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "cross-env NODE_ENV=production concurrently --raw \"start-storybook --port 6060\" \"npm:watch --silent\"",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o storybook-dist",
    "test": "jest src/",
    "watch": "concurrently --raw \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:prettier": "onchange --delay 500 \"src/**/*.scss\" -- npm run lint:scss --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent",
    "watch:test": "jest --watch src/"
  },
  "version": "1.0.0"
}
