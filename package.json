{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^2.19.17",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify-cms": "^4.1.40",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.51",
    "gatsby-transformer-sharp": "^2.3.14",
    "netlify-cms-app": "^2.11.19",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "babel-preset-gatsby": "^0.2.29",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "current-git-branch": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "onchange": "^6.1.0",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^13.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env FORCE_COLOR=true concurrently --raw \"npm:test\" \"lint-staged\"",
      "pre-push": "node ./scripts/branchCheck.js"
    }
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern /src/vendor/*",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss --color --allow-empty-input --fix",
      "git add"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "concurrently --raw \"npm:lint --silent\" \"npm:test\" && gatsby build",
    "develop": "cross-env NODE_ENV=development concurrently --raw \"npm:lint --silent\" \"gatsby develop --port 3000\" \"npm:watch --silent\"",
    "lint": "concurrently --raw \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:fix": "concurrently --raw \"eslint ./*.js src/**/*.js .storybook/*.js --fix\" \"stylelint src/**/*.scss --syntax scss --color --allow-empty-input --fix\"",
    "lint:js": "eslint ./*.js src/**/*.js .storybook/*.js --ignore-pattern /src/vendor/*",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --color --allow-empty-input",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "cross-env NODE_ENV=production concurrently --raw \"start-storybook --port 6060\" \"npm:watch --silent\"",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o storybook-dist",
    "test": "jest src/",
    "watch": "concurrently --raw \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent",
    "watch:test": "jest --watch src/"
  },
  "version": "1.0.0"
}
