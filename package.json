{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^2.28.0",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.7.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-netlify-cms": "^4.6.0",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-postcss": "^3.3.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-plugin-sharp": "^2.10.0",
    "gatsby-remark-images": "^3.7.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-remark": "^2.12.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "netlify-cms-app": "^2.14.3",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.3.1"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.1.9",
    "@storybook/addons": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "babel-preset-gatsby": "^0.8.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "current-git-branch": "^1.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "ftp": "^0.3.10",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "node-sass": "^4.14.1",
    "onchange": "^7.1.0",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env FORCE_COLOR=true concurrently --raw \"npm:test\" \"lint-staged\"",
      "pre-push": "node ./scripts/branchCheck.js"
    }
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern /src/vendor/*"
    ],
    "*.scss": [
      "stylelint --syntax scss --color --allow-empty-input --fix"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "concurrently --raw \"npm:lint --silent\" \"npm:test\" && gatsby build",
    "build:deploy": "npm run build && npm run copy",
    "copy": "copyfiles .htaccess public",
    "deploy": " node -r dotenv/config ./scripts/deploy.js",
    "develop": "cross-env NODE_ENV=development cross-env GATSBY_GRAPHQL_IDE=playground concurrently --raw \"npm:lint --silent\" \"gatsby develop --port 3000\" \"npm:watch --silent\"",
    "lint": "concurrently --raw \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:fix": "concurrently --raw \"eslint ./*.js src/**/*.js .storybook/*.js --fix\" \"stylelint src/**/*.scss --syntax scss --color --allow-empty-input --fix\"",
    "lint:js": "eslint ./*.js src/**/*.js .storybook/*.js --ignore-pattern /src/vendor/*",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --color --allow-empty-input",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "cross-env NODE_ENV=production concurrently --raw \"start-storybook --port 6060\" \"npm:watch --silent\"",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o storybook-dist",
    "test": "jest src/",
    "watch": "concurrently --raw \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent",
    "watch:test": "jest --watch src/"
  },
  "version": "1.0.0"
}
