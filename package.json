{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^2.15.34",
    "gatsby-image": "^2.2.25",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-offline": "^3.0.13",
    "gatsby-plugin-postcss": "^2.1.11",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sass": "^2.1.18",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-sharp": "^2.2.21",
    "node-sass": "^4.12.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-notes": "^5.2.3",
    "@storybook/addon-viewport": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "babel-preset-gatsby": "^0.2.18",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "current-git-branch": "^1.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "onchange": "^6.1.0",
    "react-test-renderer": "^16.10.2",
    "sass-lint": "^1.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env FORCE_COLOR=true concurrently --raw \"npm:test\" \"lint-staged\"",
      "pre-push": "node ./scripts/branchCheck.js"
    }
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern /src/vendor/*",
      "git add"
    ],
    "*.scss": [
      "sass-lint -v -q --ignore src/vendor/*",
      "git add"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "concurrently --raw \"npm:lint --silent\" \"npm:test\" && gatsby build",
    "develop": "concurrently --raw \"npm:lint --silent\" \"gatsby develop --port 3000\" \"npm:watch --silent\"",
    "lint:fix": "eslint ./*.js src/**/*.js .storybook/*.js --fix",
    "lint": "concurrently --raw \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:js": "eslint ./*.js src/**/*.js .storybook/*.js --ignore-pattern /src/vendor/*",
    "lint:scss": "sass-lint -v -q src/**/*.scss --ignore src/vendor/*",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "cross-env NODE_ENV=production concurrently --raw \"start-storybook --port 6060\" \"npm:watch --silent\"",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o storybook-dist",
    "test": "jest src/",
    "watch": "concurrently --raw \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent"
  },
  "version": "1.0.0"
}
