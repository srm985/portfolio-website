{
  "author": "Sean McQuay",
  "dependencies": {
    "gatsby": "^2.4.2",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.36",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-sharp": "^2.1.19",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "description": "Personal portfolio website V2.",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "current-git-branch": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "onchange": "^5.2.0",
    "sass-lint": "^1.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env FORCE_COLOR=true lint-staged",
      "pre-push": "node ./scripts/branchCheck.js"
    }
  },
  "keywords": [
    "portfolio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "sass-lint -v -q",
      "git add"
    ]
  },
  "name": "portfolio-website",
  "private": false,
  "scripts": {
    "build": "npm run lint --silent && gatsby build",
    "develop": "concurrently \"npm:lint --silent\" \"gatsby develop\" \"npm:lint --silent\"",
    "lint": "concurrently \"npm:lint:js --silent\" \"npm:lint:scss --silent\"",
    "lint:js": "eslint src/**/*.js",
    "lint:scss": "sass-lint -v -q src/**/*.scss",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "watch": "concurrently \"npm:watch:js --silent\" \"npm:watch:scss --silent\"",
    "watch:js": "onchange \"src/**/*.js\" -- npm run lint:js --silent",
    "watch:scss": "onchange \"src/**/*.scss\" -- npm run lint:scss --silent"
  },
  "version": "1.0.0"
}
